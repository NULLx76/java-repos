<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.wrj</groupId>
    <artifactId>spring-internal-parent</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>spring-internal-parent</name>
    <url>http://maven.apache.org</url>

    <modules>
        <module>spring-bean-internal</module>
        <module>spring-web-internal</module>
        <module>spring-internal-data</module>
        <module>spring-tx-internal</module>
    </modules>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <spring-version>5.3.16</spring-version>
        <spring-data-version>2.6.3</spring-data-version>
        <spring-internal-version>1.0.0-SNAPSHOT</spring-internal-version>
        <junit-version>4.12</junit-version>
        <mybatis-version>3.5.9</mybatis-version>
        <mybatis-spring-version>2.0.7</mybatis-spring-version>
        <h2-version>1.4.196</h2-version>
        <druid-version>1.2.8</druid-version>
        <commons-lang3-version>3.5</commons-lang3-version>
        <aspectj-version>1.8.9</aspectj-version>
        <tomcat-version>9.0.58</tomcat-version>
        <validation-api-version>2.0.0.Final</validation-api-version>
        <fast-json-version>1.2.80</fast-json-version>
    </properties>

    <distributionManagement>
        <snapshotRepository>
            <id>nexus-snapshots</id>
            <name>Nexus Snapshot Repository</name>
            <url>http://223.252.220.134:8081/nexus/content/repositories/snapshots/</url>
        </snapshotRepository>
    </distributionManagement>


    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.springframework</groupId>
