<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>GameLibrary</groupId>
  <artifactId>GameLibrary</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>war</packaging>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.5.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>3.0.0</version>
        <configuration>
          <warSourceDirectory>WebContent</warSourceDirectory>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
  		<dependency>
  				<groupId>Carver-Libraries</groupId>
  				<artifactId>JSF-MyFaces-PrimeFaces</artifactId>
  				<version>JSF 2.2 Primefaces 5</version>
  				<type>pom</type>
  		</dependency>
  		<dependency>
  				<groupId>Carver-Libraries</groupId>
  				<artifactId>Spring-Hibernate</artifactId>
  				<version>Spring 3.2 Hibernate 3.6</version>
  				<type>pom</type>
  		</dependency>
  		<dependency>
  				<groupId>Carver-Libraries</groupId>
  				<artifactId>CDI-Weld</artifactId>
  				<version>1.2</version>
  				<type>pom</type>
  		</dependency>
  		<dependency>
  				<groupId>Carver-Libraries</groupId>
  				<artifactId>Apache-Commons</artifactId>
  				<version>1.0</version>
  				<type>pom</type>
  		</dependency>
  		<dependency>
  				<groupId>Carver</groupId>
  				<artifactId>CarverJSFUtils</artifactId>
  				<version>1.0</version>
  		</dependency>
  </dependencies>
</project>