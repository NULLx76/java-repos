<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.alphasystem.arabic</groupId>
	<artifactId>arabic-keyboard-ui</artifactId>
	<packaging>jar</packaging>
	<name>arabic-keyboard-ui</name>
	<parent>
		<artifactId>jar</artifactId>
		<groupId>com.alphasystem.project</groupId>
		<version>3.0.0-SNAPSHOT</version>
		<relativePath>../project/jar/pom.xml</relativePath>
	</parent>
	<dependencies>
		<dependency>
			<groupId>com.alphasystem.arabic</groupId>
			<artifactId>arabic-support</artifactId>
			<version>${arabic-support.version}</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
	<properties>
		<arabic-support.version>${project.version}</arabic-support.version>
		<commons-ui.version>${project.version}</commons-ui.version>
		<maven.test.skip>true</maven.test.skip>
		<mainClass>com.alphasystem.arabickeyboard.ui.KeyboardApp</mainClass>
	</properties>
</project>
