  <version>1.0-SNAPSHOT</version>
  <name>learn-project Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <modules>
    <module>learn-jersey</module>
    <module>learn-freemarker</module>
    <module>learn-springsession</module>
    <module>learn-spring</module>
