<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.soapui_action_add_request_parameters</groupId>
  <artifactId>soapui-action-add-request-parameters</artifactId>
  <packaging>jar</packaging>
  <version>0.1</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <name>Add SoapUI Request Parameters Action</name>
  <url></url>
  <dependencies>
    <!-- https://mvnrepository.com/artifact/com.github.redfish4ktc.soapui/soapui -->
    <dependency>
      <groupId>com.github.redfish4ktc.soapui</groupId>
      <artifactId>soapui</artifactId>
      <version>4.0.1.0</version>
    </dependency>
  </dependencies>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
