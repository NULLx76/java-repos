<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:anna="http://maven.apache.org/POM/4.0.0"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <anna:modelVersion>4.0.0</anna:modelVersion>

    <anna:groupId>annaProg</anna:groupId>
    <anna:artifactId>flats</anna:artifactId>
    <anna:version>1.0-SNAPSHOT</anna:version>

    <anna:properties>
        <anna:project.build.sourceEncoding>UTF-8</anna:project.build.sourceEncoding>
    </anna:properties>

    <anna:dependencies>
        <anna:dependency>
            <anna:groupId>mysql</anna:groupId>
            <anna:artifactId>mysql-connector-java</anna:artifactId>
            <anna:version>5.5.54</anna:version>
        </anna:dependency>
    </anna:dependencies>

    <anna:build>
        <anna:plugins>
            <anna:plugin>
                <anna:groupId>org.apache.maven.plugins</anna:groupId>
                <anna:artifactId>maven-compiler-plugin</anna:artifactId>
                <anna:configuration>
                    <anna:source>1.7</anna:source>
                    <anna:target>1.7</anna:target>
                </anna:configuration>
            </anna:plugin>
        </anna:plugins>
    </anna:build>

</project>