	<name>Ariah Continuity</name>
	<url>http://ariahgroup.org/product/ariah-continuity/</url>
	
	<scm>
		<url>https://github.com/Ariah-Group/Continuity</url>
		<connection>scm:git:git://github.com/Ariah-Group/Continuity.git</connection>
		<developerConnection>scm:git:git@github.com:Ariah-Group/Continuity.git</developerConnection>
	</scm>

	<organization>
		<name>The Ariah Group</name>
		<url>http://ariahgroup.org</url>
	</organization>
 
	<build>
		<finalName>continuity</finalName>
		<plugins>			
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.19</version>
				<configuration>
					<skipTests>true</skipTests>
				</configuration>
			</plugin>

			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>gwt-maven-plugin</artifactId>
				<version>1.1</version>
				<executions>
					<execution>
						<configuration>
							<modules>
								<module>org.kuali.continuity.admin.main.ActivityLog</module>
								<module>org.kuali.continuity.admin.project.ImageViewer</module>
								<module>org.kuali.continuity.admin.main.Application</module>
								<module>org.kuali.continuity.admin.main.Acronym</module>
								<module>org.kuali.continuity.admin.main.Building</module>
								<module>org.kuali.continuity.admin.main.Reference</module>
								<module>org.kuali.continuity.admin.main.CriticalFunction</module>
								<module>org.kuali.continuity.admin.main.Dependency</module>
								<module>org.kuali.continuity.admin.main.Faq</module>
								<module>org.kuali.continuity.admin.main.Location</module>
								<module>org.kuali.continuity.admin.main.MajorDivision</module>
								<module>org.kuali.continuity.admin.main.Skill</module>
								<module>org.kuali.continuity.admin.main.Setting</module>
							</modules>
						</configuration>
						<goals>
							<goal>compile</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<webResources>
						<resource>
							<filtering>true</filtering>
							<directory>src/main/webapp</directory>
								<includes>
									<include>**/common/Footer.jsp</include>
									<include>**/common/LoginFooter.jsp</include>
								</includes>
						</resource>
					</webResources>
				</configuration>
			</plugin>

            <plugin>
                <groupId>org.apache.tomcat.maven</groupId>
                <artifactId>tomcat6-maven-plugin</artifactId>
                <version>2.1</version>
                <dependencies>
					<dependency>
						<groupId>net.sourceforge.jtds</groupId>
						<artifactId>jtds</artifactId>
						<version>${version.jtds}</version>
					    <scope>runtime</scope>
					</dependency>
					<dependency>
						<groupId>javax.mail</groupId>
						<artifactId>mail</artifactId>
						<version>${version.mail}</version>
						<scope>runtime</scope>
					</dependency>
                </dependencies>
                <configuration>
                    <port>8080</port>
                    <path>/continuity</path>
                </configuration>
            </plugin>

		</plugins>

		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>

	</build>

