<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.chemaxon</groupId>
    <artifactId>ChemistryToolkitMarvin</artifactId>
    <version>15.5.4.0</version>
    <packaging>jar</packaging>
    <name>ChemistryToolkitMarvin</name>
    <repositories>
        <repository>
            <id>ChemAxon Public Repository</id>
            <url>https://repository.chemaxon.com/artifactory/libs-release</url>
        </repository>
    </repositories>
	<dependencies>
		<dependency>
			<groupId>org.helm</groupId>
			<artifactId>ChemistryToolkit</artifactId>
			<version>1.0.0-SNAPSHOT</version>
		</dependency>
        <dependency>
            <groupId>com.chemaxon</groupId>
            <artifactId>jchem</artifactId>
            <version>15.5.4.0</version>
        </dependency>
		<dependency>
			<groupId>org.testng</groupId>
			<artifactId>testng</artifactId>
			<version>6.9.10</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-api</artifactId>
			<version>1.7.12</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-log4j12</artifactId>
			<version>1.7.12</version>
		</dependency>
	</dependencies>
	<organization>
		<name>ChemAxon</name>
		<url>http://www.chemaxon.com/</url>
	</organization>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
  <distributionManagement>
    <repository>
      <id>ChemAxon Public Repository</id>
      <name>ChemAxon Public Repository</name>
      <url>https://repository.chemaxon.com/artifactory/ext-release-local</url>
    </repository>
  </distributionManagement>
</project>
