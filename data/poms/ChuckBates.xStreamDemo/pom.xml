<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.fibi.plugins</groupId>
    <artifactId>xStreamDemo</artifactId>
    <version>1.0</version>
    <properties>
        <version.fishbowl>16.0</version.fishbowl>
    </properties>

    <build>
        <defaultGoal>install</defaultGoal>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.3</version>
                <configuration>
                    <source>1.7</source>
                    <target>1.7</target>
                    <fork>false</fork>
                    <optimize>true</optimize>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>client</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>client-commons</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>eve</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>eve-commons</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>fbcore</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>fbdata</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>fbifbo</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>fbconstants</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>figui</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>fpo</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>util</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>elfgr</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.fbi</groupId>
            <artifactId>fbswing</artifactId>
            <version>${version.fishbowl}</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>

    <repositories>
        <repository>
            <id>fishbowl-snapshot</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/Fishbowl</url>
        </repository>
        <repository>
            <id>local-releases</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/releases</url>
        </repository>
        <repository>
            <id>central</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/central</url>
        </repository>
        <repository>
            <id>apache-release</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/apache-releases</url>
        </repository>
        <repository>
            <id>mvnrepository</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/mvnrepository</url>
        </repository>
        <repository>
            <id>apache-snapshots</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/apache-snapshots</url>
        </repository>
        <repository>
            <id>codehaus-snapshots</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/codehaus-snapshots</url>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>fishbowl-snapshot</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/Fishbowl</url>
        </pluginRepository>
        <pluginRepository>
            <id>local-releases</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/releases</url>
        </pluginRepository>
        <pluginRepository>
            <id>central</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/central</url>
        </pluginRepository>
        <pluginRepository>
            <id>apache-release</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/apache-releases</url>
        </pluginRepository>
        <pluginRepository>
            <id>mvnrepository</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/mvnrepository</url>
        </pluginRepository>
        <pluginRepository>
            <id>apache-snapshots</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/apache-snapshots</url>
        </pluginRepository>
        <pluginRepository>
            <id>codehaus-snapshots</id>
            <url>http://murky.fishbowldev.com:8081/nexus/content/repositories/codehaus-snapshots</url>
        </pluginRepository>
    </pluginRepositories>
</project>