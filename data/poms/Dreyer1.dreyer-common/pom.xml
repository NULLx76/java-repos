    <modelVersion>4.0.0</modelVersion>
    <artifactId>dreyer-common</artifactId>
    <packaging>jar</packaging>
    <version>${dreyer-common.version}</version>
    <name>dreyer-common</name>
    <url>http://maven.apache.org</url>

    <parent>
        <groupId>com.dreyer.common</groupId>
        <artifactId>dreyer-common-parent</artifactId>
        <relativePath>../dreyer-common-parent</relativePath>
