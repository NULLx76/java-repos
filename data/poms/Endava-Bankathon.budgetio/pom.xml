<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>budgetio</groupId>
	<artifactId>budgetio-repo-parent</artifactId>
	<version>1.0</version>
	<packaging>pom</packaging>
	<name>BudgetIo Repository Parent</name>
	<properties>
		<!-- Figo SDK versions -->
		<figo.sdk.version>1.2</figo.sdk.version>
		<google.gson.version>2.3.1</google.gson.version>
		<!-- Maven versions -->
		<maven-dependency-plugin.version>2.10</maven-dependency-plugin.version>
		<maven-clean-plugin.version>2.6.1</maven-clean-plugin.version>
		<!-- Apache versions -->
		<apache.commons.io.version>2.4</apache.commons.io.version>
		<apache.commons.lang.version>2.6</apache.commons.lang.version>
		<apache.cxf.version>3.0.4</apache.cxf.version>
		<!-- Jersey version -->
		<jersey.version>2.17</jersey.version>
		<!-- Jackson version -->
		<jackson.version>2.0.1</jackson.version>
		<!-- Spring version -->
		<spring.version>4.1.0.RELEASE</spring.version>
		<!-- Java applications dependencies versions -->
		<maven.compiler.source>1.7</maven.compiler.source>
		<maven.compiler.target>1.7</maven.compiler.target>
		<jdk.version>1.7</jdk.version>
	</properties>

	<modules>
		<module>budgetio</module>
	</modules>

</project>
