<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.jevis.jecommons</groupId>
  <artifactId>DataProcessing</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>DataProcessing</name>
  <url>http://maven.apache.org</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

  <repositories>
        <repository>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
                <checksumPolicy>fail</checksumPolicy>
            </releases>
            <id>env1</id>
            <name>envidatec</name>
            <url>http://alpha.openjevis.org/maven2/</url>
            <layout>default</layout>
        </repository>
  </repositories>
  <dependencies>
        <dependency>
            <groupId>org.jevis.api.sql</groupId>
            <artifactId>jeapi-sql</artifactId>    
            <version>3.0.1</version>
            <type>jar</type>
        </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.10</version>
      <scope>test</scope>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.jevis.api</groupId>
      <artifactId>jeapi</artifactId>
      <version>3.0.1</version>
    </dependency>
    <dependency>
      <groupId>org.jevis.commons</groupId>
      <artifactId>JECommons</artifactId>
      <version>3.0</version>
    </dependency>
    <dependency>
      <groupId>org.jevis.application</groupId>
      <artifactId>jeapplication</artifactId>
      <version>3.0.0</version>
    </dependency>
    
    <dependency>
    <groupId>joda-time</groupId>
    <artifactId>joda-time</artifactId>
    <version>2.3</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.javis.jecommons</groupId>
      <artifactId>DataProcessing</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.javis.jecommons</groupId>
      <artifactId>DataProcessing-Interface</artifactId>
      <version>1.0-SNAPSHOT</version>
      <type>jar</type>
    </dependency>
  </dependencies>
  
  <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
