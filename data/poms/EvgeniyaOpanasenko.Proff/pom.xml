        </dependency>

    </dependencies>

    <properties>
        <java.version>1.8</java.version>
        <hibernate-version>5.2.1.Final</hibernate-version>
        <spring-version>4.1.4.RELEASE</spring-version>
        <springsecurity.version>4.0.4.RELEASE</springsecurity.version>
    </properties>



</project>