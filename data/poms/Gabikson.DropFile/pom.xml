<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>dropfile</groupId>
    <artifactId>DropFile</artifactId>
    <packaging>war</packaging>
    <version>1.0</version>
    <name>DropFile</name>
    <url>http://maven.apache.org</url>

    <properties>
        <junit.version>4.12</junit.version>
        <spring.version>4.2.5.RELEASE</spring.version>
        <spring-security.version>4.0.4.RELEASE</spring-security.version>
        <spring-web.version>4.2.5.RELEASE</spring-web.version>
        <log4j.version>1.2.17</log4j.version>
        <java-mail.version>1.5.3</java-mail.version>
        <liquibase.version>3.5.0</liquibase.version>
        <hibernate.version>5.0.1.Final</hibernate.version>
        <thymeleaf.version>2.1.2.RELEASE</thymeleaf.version>
        <quartz.version>2.2.3</quartz.version>
    </properties>

    <build>
        <finalName>DropFile</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
