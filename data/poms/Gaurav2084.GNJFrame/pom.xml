  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.4</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
          <executable>${env.JAVA_HOME}/bin/javac</executable>
          <fork>true</fork>
        </configuration>
      </plugin>

    </plugins>
  </build>

  <!-- Profiles to run integration tests on different targets -->
  <profiles>
    <!-- Firefox 3 -->
    <profile>
      <id>FF_3</id>
      <build>
        <plugins>
          <plugin>
            <artifactId>maven-antrun-plugin</artifactId>
            <version>1.6</version>
            <executions>
              <execution>
                <phase>process-sources</phase>
                <configuration>
                  <target>
                    <copy
                      file="src/test/resources/env.properties"
                      tofile="src/main/resources/Config.properties"
                      overwrite="true" />
                  </target>
                </configuration>
                <goals>
                  <goal>run</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-surefire-plugin</artifactId>
            <version>2.15</version>
            <configuration>
              <forkCount>1</forkCount>
			  <reuseForks>false</reuseForks>
			  <parallel>classes</parallel>
              <threadCount>1</threadCount>
              <includes>
                <include>**/*Test.java</include>
              </includes>
              <excludes>
                <exclude>**/backlog/*.java</exclude>
              </excludes>
              <argLine>-Dbaseurl.App=http://127.0.0.1
                -Dcontext.App.path=/login.html -Dhttp.App.port=80
                -Dwebdriver.browser=firefox_3:url=http://127.0.0.1:5000/wd/hub</argLine>
              <systemPropertyVariables>
                <browser.name>firefox</browser.name>
                <browser.version>3</browser.version>
                <browser.platform>WINDOWS</browser.platform>
              </systemPropertyVariables>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>

