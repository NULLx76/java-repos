<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>GlobalCompuMegaNett</groupId>
  <artifactId>Almacenator</artifactId>
  <packaging>jar</packaging>
  <version>2.7</version>
  <name>Almacenator</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>


  <build>
    <directory>target</directory>
    <outputDirectory>target/classes/Almacenator</outputDirectory>
    <finalName>${artifactId}-${version}</finalName>
    <testOutputDirectory>target/test-classes/Almacenator</testOutputDirectory>
    <sourceDirectory>src/main/java/Almacenator</sourceDirectory>
    <scriptSourceDirectory>src/main/scripts</scriptSourceDirectory>
    <testSourceDirectory>src/test/java/Almacenator</testSourceDirectory>
    <resources>
      <resource>
        <directory>src/main/resources/Almacenator</directory>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>src/test/resources/Almacenator</directory>
      </testResource>
    </testResources>
  </build>

</project>
