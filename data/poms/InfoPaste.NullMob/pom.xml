<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>NullMob</groupId>
    <artifactId>NullMob</artifactId>
    <version>1.0</version>

    <repositories>
        <!-- Wordguard/Wordedit Repo-->
        <repository>
            <id>sk89q-repo</id>
            <url>http://maven.sk89q.com/repo/</url>
        </repository>

        <!-- Spigot Repo -->
        <repository>
            <id>spigot-repo</id>
            <url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/</url>
        </repository>
    </repositories>

    <dependencies>
        <!--Spigot API-->
        <dependency>
            <groupId>org.spigotmc</groupId>
            <artifactId>spigot-api</artifactId>
            <version>1.9.4-R0.1-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>

        <!-- Craftbukkit API 1.10-->
        <dependency>
            <groupId>craftbukkitR3</groupId>
            <artifactId>local.craftbukkit</artifactId>
            <version>craftbukkit-1.10-R0.1-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/craftbukkit-1.10.jar</systemPath>
        </dependency>

        <!-- Craftbukkit API 1.9.4-->
        <dependency>
            <groupId>craftbukkitR2</groupId>
            <artifactId>local.craftbukkit</artifactId>
            <version>craftbukkit-1.9.4-R0.1-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/craftbukkit-1.9.4.jar</systemPath>
        </dependency>

        <!-- Craftbukkit API 1.9.2-->
        <dependency>
            <groupId>craftbukkitR1</groupId>
            <artifactId>local.craftbukkit</artifactId>
            <version>craftbukkit-1.9.2-R0.1-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/craftbukkit-1.9.2.jar</systemPath>
        </dependency>

        <!--Bukkit API-->
        <dependency>
            <groupId>org.bukkit</groupId>
            <artifactId>bukkit</artifactId>
            <version>1.9.4-R0.1-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>

        <!-- WorldGuard API-->
        <dependency>
            <groupId>com.sk89q</groupId>
            <artifactId>worldguard</artifactId>
            <version>6.0.0-SNAPSHOT</version>
        </dependency>

    </dependencies>

    <build>
        <directory>${dir}</directory>
    </build>

    <properties>
        <maven.compiler.source>1.7</maven.compiler.source>
        <maven.compiler.target>1.7</maven.compiler.target>
    </properties>
</project>