<distributionManagement>
    <repository>
        <id>release</id>
        <name>Pavans-MacBook-Pro.local-releases</name>
        <url>http://localhost:8080/artifactory/cox-test</url>
    </repository>
    <snapshotRepository>
        <id>snapshots</id>
        <name>Pavans-MacBook-Pro.local-snapshots</name>
        <url>http://localhost:8080/artifactory/cox-test</url>
    </snapshotRepository>
</distributionManagement>    
<build>
  <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.3</version>
      </plugin>
<plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.5.1</version>
                <executions>
                <execution>
                    <id>make-bundles</id>
                    <phase>package</phase>
                    <goals>
                    <goal>single</goal>
                    </goals>
                <configuration>
                    <!-- Configures the used assembly descriptor -->
                    <descriptors>
                        <descriptor>src/main/assembly/assembly.xml</descriptor>
                    </descriptors>
                </configuration>
                </execution>
        </executions>
            </plugin>
    </plugins>
