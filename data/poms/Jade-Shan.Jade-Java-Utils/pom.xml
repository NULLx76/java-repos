<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/5.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>net.jade-dungeon</groupId>
		<artifactId>jadelibs-java</artifactId>
		<version>1.0.14</version>
	</parent>

	<artifactId>javautils</artifactId>
	<version>2.0.0</version>
	<packaging>pom</packaging>
	<name>jade utils parent</name>

	<modules>
		<module>javautils-base</module>
		<module>javautils-cache</module>
		<module>javautils-dao</module>
		<module>javautils-mongo</module>
		<module>javautils-web</module>
		<module>javautils-sandtable</module>
	</modules>

	<properties>
		<javautils-base.version>2.0.0</javautils-base.version>
		<javautils-cache.version>2.0.0</javautils-cache.version>
		<javautils-dao.version>2.0.0</javautils-dao.version>
		<javautils-mongo.version>2.0.0</javautils-mongo.version>
		<javautils-web.version>1.0.20</javautils-web.version>
		<javautils-sandtable.version>2.0.0</javautils-sandtable.version>
		<javautils-cdn.version>2.0.0</javautils-cdn.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<!-- remove after update jade-java-libs -->
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-base</artifactId>
				<version>${javautils-base.version}</version>
			</dependency>
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-cache</artifactId>
				<version>${javautils-cache.version}</version>
			</dependency>
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-dao</artifactId>
				<version>${javautils-dao.version}</version>
			</dependency>
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-mongo</artifactId>
				<version>${javautils-mongo.version}</version>
			</dependency>
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-web</artifactId>
				<version>${javautils-web.version}</version>
			</dependency>
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-sandtable</artifactId>
				<version>${javautils-sandtable.version}</version>
			</dependency>
			<dependency>
				<groupId>net.jade-dungeon</groupId>
				<artifactId>javautils-cdn</artifactId>
				<version>${javautils-cdn.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

	<build>
	</build>

</project>  
