<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
         http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.jboss.arquillian.showcase</groupId>
        <artifactId>arquillian-showcase-spring</artifactId>
        <version>1.0.0-SNAPSHOT</version>
        <relativePath>../../documents/arquillian/arquillian-showcase/spring/pom.xml</relativePath>
    </parent>
    <groupId>net.khaled-noordin.functional-test</groupId>
    <artifactId>functional-test</artifactId>
    <version>0.0.0</version>
    <packaging>pom</packaging>
    <name>functional-test</name>
    <modules>
        <module>arquillian</module>
        <module>arquillian-groovy</module>
    </modules>
    <properties>
        <core.version>0.0.0</core.version>
        <frontside.version>0.0.0</frontside.version>
        <organization.version>0.0.0</organization.version>
        <endorsed.dir>${project.build.directory}/endorsed</endorsed.dir>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <groovy.version>2.1.8</groovy.version>
        <slf4j.version>1.7.5</slf4j.version>
        <logback.version>1.0.13</logback.version>
        <junit.version>4.11</junit.version>
        <spock.version>0.7-groovy-2.0</spock.version>
        <hibernate.version>4.2.5.Final</hibernate.version>
        <hibernate-validator.version>5.0.1.Final</hibernate-validator.version>
        <maven-surefire-plugin.version>2.16</maven-surefire-plugin.version>
        <maven-dependency-plugin.version>2.8</maven-dependency-plugin.version>
        <groovy-eclipse-compiler.version>2.8.0-01</groovy-eclipse-compiler.version>
        <groovy-eclipse-batch.version>2.1.5-03</groovy-eclipse-batch.version>
        <groovy-eclipse-compiler.version>2.8.0-01</groovy-eclipse-compiler.version>
        <maven-compiler-plugin.version>3.1</maven-compiler-plugin.version>
        <maven-checkstyle-plugin.version>2.10</maven-checkstyle-plugin.version>
        <maven-eclipse-plugin.version>2.9</maven-eclipse-plugin.version>
        <java.source.version>1.7</java.source.version>
        <java.target.version>1.7</java.target.version>
        <eclipse-wtp.version>2.0</eclipse-wtp.version>
        <jackson.version>1.9.13</jackson.version>
        <spring.version>3.2.4.RELEASE</spring.version>
        <swf.version>2.4.0.M1</swf.version>
        <javax.el.version>2.2.5</javax.el.version>
        <cdi-api.version>1.1-PRD</cdi-api.version>
        <dbunit.version>2.4.9</dbunit.version>
        <mock-javamail.version>1.9</mock-javamail.version>
        <aspectj.version>1.7.3</aspectj.version>
        <cglib.version>3.0</cglib.version>
        <easymock.version>3.2</easymock.version>
        <jchronic.version>0.2.6</jchronic.version>
        <h2database.version>1.3.173</h2database.version>
        <omnifaces.version>1.6.1</omnifaces.version>
        <javax.mail.version>1.4.7</javax.mail.version>
        <primefaces.themes.version>1.0.10</primefaces.themes.version>
        <mysql-connector-java.version>5.1.26</mysql-connector-java.version>
        <maven-war-plugin.version>2.3</maven-war-plugin.version>
        <tomcat-maven-plugin.version>2.1</tomcat-maven-plugin.version>
        <tomee-maven-plugin.version>1.0.0</tomee-maven-plugin.version>
        <tomee.version>1.5.0</tomee.version>
        <javax.faces.version>2.2.4</javax.faces.version>
        <primefaces.version>4.0</primefaces.version>
        <spring-sec.version>3.1.4.RELEASE</spring-sec.version>
        <twitter-bootstrap.version>3.0.0</twitter-bootstrap.version>
        <hsqldb.version>2.3.1</hsqldb.version>
        <log4j.version>1.2.17</log4j.version>
        <javax.servlet.version>3.1.0</javax.servlet.version>
        <javax.servlet.jsp.jstl.version>1.2</javax.servlet.jsp.jstl.version>
        <ehcache.version>2.6.6</ehcache.version>
        <commons-fileupload.version>1.3</commons-fileupload.version>
        <maven-resources-plugin.version>2.6</maven-resources-plugin.version>
        <java.compiler.version>1.7</java.compiler.version>
    </properties>

    <repositories>
        <repository>
            <id>JBoss Repository</id>
            <name>JBoss Repository</name>
            <url>http://repository.jboss.org/nexus/content/groups/public-jboss/</url>
        </repository>
        <repository>
            <id>snapshots.jboss.org</id>
            <name>JBoss Snapshot Repository</name>
            <url>http://snapshots.jboss.org/maven2</url>
            <releases>
                <enabled>false</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>repository.springsource.maven.release</id>
            <name>SpringSource Repository</name>
            <url>http://repo.springsource.org/release</url>
        </repository>
        <repository>
            <id>spring-repository</id>
            <name>SpringSource Repository</name>
            <url>http://repo.springsource.org/milestone</url>
        </repository>
        <repository>
            <id>primefaces-repository</id>
            <name>Prime Technology Maven Repository</name>
            <url>http://repository.primefaces.org</url>
            <layout>default</layout>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>repository.springsource.maven.milestone</id>
            <name>Spring Framework Maven Milestone Repository</name>
            <url>http://repo.springsource.org/milestone</url>
        </pluginRepository>
        <pluginRepository>
            <id>snapshots.jboss.org</id>
            <name>JBoss Snapshot Repository</name>
            <url>http://snapshots.jboss.org/maven2</url>
            <releases>
                <enabled>false</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </pluginRepository>
    </pluginRepositories>
</project>