<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/maven-v4_0_0.xsd">

	<!-- Begin project description -->
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.coldcore</groupId>
	<artifactId>coldcore-all</artifactId>
	<version>1.0</version>
	<name>coldcore-all</name>
	<packaging>pom</packaging>
	<organization>
		<name>ColdCore</name>
		<url>http://coldcore.com</url>
	</organization>
	<url>http://coldcore.com</url>
	<modules>
		<module>coloradoftp</module>
		<module>misc5</module>
	</modules>
	<!-- End project description -->

	<!-- Begin source control configuration -->
	<scm>
		<connection>scm:svn:http://denverftp.googlecode.com/svn/trunk</connection>
		<developerConnection>scm:svn:http://denverftp.googlecode.com/svn/trunk</developerConnection>
		<url>http://denverftp.googlecode.com</url>
	</scm>
	<!-- End source control configuration -->

	<!-- Begin build configuration -->
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<compilerVersion>1.6</compilerVersion>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<!-- End build configuration -->

	<!-- Begin report configuration -->
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<inherited>true</inherited>
			</plugin>
		</plugins>
	</reporting>
	<!-- End report configuration -->

</project>
