  <groupId>com.rw</groupId>
  <artifactId>DemoSetup</artifactId>
  <version>V1</version>
  <packaging>jar</packaging>

  <name>ReferralWire DemoData Setup</name>
  <url>http://www.referralwire.com</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <skipTests>true</skipTests>
  </properties>
 <build>
         <resources>
            <resource>
                <directory>${basedir}</directory>
                <includes>
                    <include>log4j.properties</include>
                 </includes>
            </resource>
        </resources>
  
 	<plugins>
             <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>2.4</version>
                <configuration>
					<archive>
                    	<manifest>
                            <mainClass>com.rw.App</mainClass>
                            <addClasspath>true</addClasspath>
                        </manifest>
                 	</archive>
                </configuration>
            </plugin>
  	</plugins>          
 </build>
