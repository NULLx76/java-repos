<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>br.com.xavier.suricate.dbms</groupId>
	<artifactId>SuricateDBMS</artifactId>
	<version>0.1b</version>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		
		<!-- GITHUB DEPENCIES -->
		<!-- DOWNLOAD AND INSTALL IN MAVEN  -->
	
		<!-- GRAPHS LIBRARY -->
		<!-- https://github.com/MatheusArleson/Graphs -->
		<dependency>
			<groupId>br.com.xavier</groupId>
			<artifactId>Graphs</artifactId>
			<version>1.7.6</version>
		</dependency>
		
		<!-- MATRIX LIBRARY -->
		<!-- https://github.com/MatheusArleson/Matrix -->
		<dependency>
			<groupId>br.com.xavier</groupId>
			<artifactId>Matrix</artifactId>
			<version>1.2.0</version>
		</dependency>
		
		<!-- GRAPHS CYTOSCAPE LIBRARY -->
		<!-- https://github.com/MatheusArleson/GraphsCytoscapeParser -->
		<dependency>
			<groupId>br.com.xavier</groupId>
			<artifactId>GraphsCytoscapeParser</artifactId>
			<version>1.4.1</version>
		</dependency>

		<!-- END OF GITHUB DEPENCIES -->

		<!-- MAVEN DEPENCIES -->
		
		<!-- https://mvnrepository.com/artifact/commons-io/commons-io -->
		<dependency>
			<groupId>commons-io</groupId>
			<artifactId>commons-io</artifactId>
			<version>2.5</version>
		</dependency>

		<!-- https://mvnrepository.com/artifact/org.apache.commons/commons-collections4 -->
		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-collections4</artifactId>
			<version>4.1</version>
		</dependency>

		<!-- https://mvnrepository.com/artifact/junit/junit -->
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.12</version>
			<scope>test</scope>
		</dependency>
		
		<!-- https://mvnrepository.com/artifact/log4j/log4j -->
		<dependency>
		    <groupId>log4j</groupId>
		    <artifactId>log4j</artifactId>
		    <version>1.2.17</version>
		</dependency>
	
		<!-- END OF MAVEN DEPENCIES -->
	</dependencies>

</project>