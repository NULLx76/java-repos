<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.minimajack._1c</groupId>
	<artifactId>com.minimajack._1c.cluster</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>Demo administration</name>
	<properties>
		<version.logback>1.0.13</version.logback>
		<version.slf4j>1.7.5</version.slf4j>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
	</properties>
	<dependencies>
		<dependency>
			<groupId>org.jboss.netty</groupId>
			<artifactId>netty</artifactId>
			<version>3.2.6.Final</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-simple</artifactId>
			<version>1.7.5</version>
		</dependency>
		<dependency>
			<groupId>com._1c.v8.core</groupId>
			<artifactId>com._1c.v8.core</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com._1c.v8.ibis</groupId>
			<artifactId>com._1c.v8.ibis</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com._1c.v8.ibis</groupId>
			<artifactId>com._1c.v8.ibis.admin</artifactId>
			<version>1.3.0</version>
		</dependency>
		<dependency>
			<groupId>com._1c.v8.ibis</groupId>
			<artifactId>com._1c.v8.ibis.swp</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com._1c.v8.swp</groupId>
			<artifactId>com._1c.v8.swp</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com._1c.v8.swp</groupId>
			<artifactId>com._1c.v8.swp.netty</artifactId>
			<version>1.0.0</version>
		</dependency>
	</dependencies>
	<description>Demo project for administration</description>
	<repositories>
		<repository>
			<id>minimajack</id>
			<url>https://raw.githubusercontent.com/MinimaJack/repository/master/</url>
		</repository>
	</repositories>
	<build>
		<sourceDirectory>src/main/java</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<version>2.5.3</version>
				<configuration>
					<archive>
						<manifest>
							<mainClass>
								sample.console.ui.AgentAdminConsole
							</mainClass>
						</manifest>
					</archive>
					<descriptorRefs>
						<descriptorRef>jar-with-dependencies</descriptorRef>
					</descriptorRefs>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>