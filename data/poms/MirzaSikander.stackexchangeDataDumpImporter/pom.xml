                        <artifactId>exec-maven-plugin</artifactId>
                        <version>1.3.2</version>
                        <configuration>
                            <executable>java</executable>
                            <mainClass>arffConversion.TrailingCommaRemover</mainClass>
                            <systemProperties>
                                <systemProperty>
