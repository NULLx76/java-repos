            <properties>
                <inputFile></inputFile>
                <outputFile></outputFile>
            </properties>
            <build>
                <pluginManagement>
                    <plugins>
                        <plugin>
                            <artifactId>maven-compiler-plugin</artifactId>
                            <version>3.2</version>
                            <configuration>
                                <source>1.8</source>
                                <target>1.8</target>
