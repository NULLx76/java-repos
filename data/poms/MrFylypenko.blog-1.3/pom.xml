          <version>5.2.2.Final</version>
      </dependency>

      <dependency>
          <groupId>org.springframework</groupId>
          <artifactId>spring-orm</artifactId>
          <version>4.3.3.RELEASE</version>
      </dependency>
  </dependencies>






  <build>
