<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <modules>
        <module>client-examples</module>
    </modules>

    <repositories>
        <repository>
            <id>ossrh</id>
            <name>ossrh Snapshots</name>
            <url>https://oss.sonatype.org/content/repositories/snapshots</url>
            <releases>
                <enabled>false</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>


    <parent>
        <groupId>io.github.cloudiator</groupId>
        <artifactId>project</artifactId>
        <version>0.2.0-SNAPSHOT</version>
    </parent>
    <artifactId>examples</artifactId>
    <packaging>pom</packaging>
    <version>0.2.0-SNAPSHOT</version>


    <name>examples</name>
    <description>Examples for cloudiator.</description>
    <url>https://github.com/cloudiator</url>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>


    <scm>
        <connection>scm:git:https://github.com/cloudiator/examples.git</connection>
        <developerConnection>scm:git:git@github.com:cloudiator/examples.git
        </developerConnection>
        <url>https://github.com/cloudiator/examples/</url>
        <tag>HEAD</tag>
    </scm>
    
</project>
