  <groupId>it.iubar.invoice</groupId>
  <artifactId>iubar_invoice</artifactId>
  <version>0.0.1</version>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
