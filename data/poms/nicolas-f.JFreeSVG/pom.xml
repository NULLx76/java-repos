<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
        <modelVersion>4.0.0</modelVersion>
        <groupId>org.jfree</groupId>
        <artifactId>jfreesvg-platform</artifactId>
        <packaging>pom</packaging>
        <version>2.1</version>
        <name>platform</name>
        <organization>
                <name>object-refinery</name>
                <url>http://www.object-refinery.com</url>
        </organization>
        <url>http://www.jfree.org/jfreechart/</url>
        <modules>
                <module>core</module>
                <module>demo</module>
        </modules>
        <properties>
                <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        </properties>
        <scm>
                <connection>scm:git:https://github.com/irstv/JFreeChart.git</connection>
                <url>https://github.com/irstv/JFreeChart</url>
        </scm>
        <repositories>
                <repository>
                        <id>repo2.maven.org</id>
                        <name>Maven2 repository 2</name>
                        <url>http://repo2.maven.org/maven2</url>
                </repository>
        </repositories>
        <build>
                <plugins>
                        <plugin>
                                <artifactId>maven-compiler-plugin</artifactId>
                                <version>2.3.2</version>
                                <configuration>
                                        <source>1.6</source>
                                        <target>1.6</target>
                                </configuration>
                        </plugin>
                </plugins>
        </build>
</project>
